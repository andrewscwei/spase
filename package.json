{
  "name": "spase",
  "version": "6.3.0",
  "description": "Micro library for performing 2D spatial calculations of DOM elements",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --config config/build.conf.ts",
    "pages": "rimraf .gh-pages && webpack --config demo/config/dev.conf.ts",
    "demo": "npm run build -- --watch & NODE_ENV=development webpack-dev-server --hot --config demo/config/dev.conf.ts --port 8080",
    "test": "echo \"No tests yet\"",
    "lint": "eslint --ext .ts src config demo",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "browser",
    "window",
    "viewport",
    "element",
    "dimension",
    "utility",
    "2d",
    "typescript",
    "rect"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewscwei/spase.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/compression-webpack-plugin": "^9.0.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^17.0.19",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "compression-webpack-plugin": "^9.2.0",
    "eslint": "^8.9.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  },
  "types": "build/index.d.ts"
}
