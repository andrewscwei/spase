{
  "name": "spase",
  "version": "8.0.4",
  "description": "Micro library for performing 2D spatial calculations of DOM elements",
  "license": "MIT",
  "main": "build/spase.js",
  "type": "module",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf -g build",
    "build": "vite build",
    "prepages": "rimraf .gh-pages",
    "pages": "vite build -c demo/vite.config.ts",
    "dev": "concurrently \"vite build -w \" \"wait-on build && vite dev -c demo/vite.config.ts --port 8080\"",
    "test": "echo \"No tests yet\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "keywords": [
    "browser",
    "window",
    "viewport",
    "element",
    "dimension",
    "utility",
    "2d",
    "typescript",
    "rect"
  ],
  "files": [
    "build"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewscwei/spase.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/node": "^22.5.4",
    "concurrently": "^9.0.0",
    "eslint": "^9.10.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.3",
    "vite-plugin-dts": "^4.2.1",
    "wait-on": "^8.0.1"
  }
}
